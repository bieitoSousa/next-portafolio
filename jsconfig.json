{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@common/*": ["src/common/*"],
      "@context/*": ["src/context/*"],
      "@moodular-ui/*": ["src/common/ui-library/*"],
      "@moodular-components/*": ["src/common/components/*"],
      "@moodular-layouts/*": ["src/common/layouts/*"],
      "@fetchers/*": ["src/fetchers/*"],
      "@hooks/*": ["src/hooks/*"],
      "@sections/*": ["src/sections/*"],
      "@utils/*": ["src/utils/*"],
      "@helpers/*": ["src/utils/helpers/*"],
      "@lib/*": ["src/utils/libs*"],
      "@styles/*": ["src/styles/*"],
      "@root/*": ["./*"]
    }
  }
}
